#include <iostream>
#include <cmath>

int main() {
  int ar [10] = {-10, 15, -1000, 129, -14, 152, -11, 2, 13, 5};
  
  for(int i = 0; i < 10; i++){
    for(int j = 0; j < 10; j++){
      if (abs(ar[i]) < abs(ar[j])){
        int temp = ar[i];
        ar[i] = ar[j];
        ar[j] = temp;
      }
    }
  }

  for (int num = 0; num < 10; num++){
    std::cout << ar[num] << "  ";
  }

}
